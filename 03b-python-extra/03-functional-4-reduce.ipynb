   "source": [
    "# Functional - Reduce\n",
    "\n",
    "reduce() applies a function to the items in an iterable two at a time, progressively combining them to produce a single result."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "15"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def f(x, y):\n",
    "    return x + y\n",
    "\n",
    "from functools import reduce\n",
    "reduce(f, [1, 2, 3, 4, 5])"
   ]
  },
